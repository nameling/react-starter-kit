    "react-dom": "^16.12.0",
    "react-router-dom": "^5.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.3",

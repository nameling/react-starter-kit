{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "react-starter-kit",
  "main": "index.js",
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "antd": "^3.19.8",
    "babel-loader": "^8.0.6",
    "history": "^4.9.0",
